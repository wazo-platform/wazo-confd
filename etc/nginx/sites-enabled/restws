server {
    listen 127.0.0.1:50050 default_server;
    server_name $domain;
    location / { try_files $uri @restws; }
    location @restws {
         include fastcgi_params;
         fastcgi_param PATH_INFO $fastcgi_script_name;
         fastcgi_param SCRIPT_NAME "";
         fastcgi_pass unix:/var/www/restws-fcgi.sock;
    }
}
